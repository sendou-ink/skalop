{
  "name": "skalop",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "bun-types": "^1.3.5"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "docker:build": "docker build -t skalop .",
    "docker:redis": "docker run -d --name skalop-redis -p 6379:6379 redis:alpine",
    "docker:start": "docker run -p ${PORT:-3000}:${PORT:-3000} -e REDIS_URL -e SKALOP_TOKEN -e SESSION_SECRET -e PORT skalop"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@remix-run/node": "^2.17.2",
    "tiny-invariant": "^1.3.3"
  }
}
